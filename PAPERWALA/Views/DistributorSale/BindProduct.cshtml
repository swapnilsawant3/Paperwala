<div class="row">
    <div class="col-sm-12">
        <div class="card">
            @*<div class="card-header d-block">
                <h3> Sale Paper List  | <span> @Html.ActionLink("Create Distributor Transaction", "Create", "DistributorTransavction", new { @class = "btn btn-info btn-default" }) </span></h3>
            </div>*@
            <div class="card-body">
                <div class="dt-responsive">
                    <table id="simpletable" class="table table-striped table-bordered">
                        <thead>
                            <tr>
                                @*<th>Sale Order</th>*@
                                <th>Paper</th>
                                <th>P-Rate</th>
                                <th>P-Quantity</th>
                                <th>P-Total</th>
                                <th>PMP Qnt</th>
                                <th>PMP AMT</th>
                                <th>Toatl PMP AMT</th>
                                <th>FINAL AMT </th>
                                
                                <th>Delete </th>
                            </tr>
                        </thead>
                        <tbody>
                            @if (Model != null)
                            {
                                foreach (var item in Model)
                                {
                                    <tr>
                                       
                                        @*<td>@item.SaleOrder</td>*@
                                        <td>@item.PaperName</td>
                                        <td>@item.PaperRate</td>
                                        <td>@item.PaperQuantity</td>
                                        <td>@item.TotalPrice</td>
                                        <td>@item.PamphletQuantity</td>
                                        <td>@item.PamphletRate</td>
                                        <td>@item.TotalPamphletAmount</td>
                                        <td>@item.TotalFinalAmount</td>
                                      
                                        <td>@Html.ActionLink("Delete", "DeleteSaleProduct", new { id = @item.DistributorSaleProductId }, new { onclick = "return confirm('Are you sure you wish to delete this Transaction?');" })</td>
                                    </tr>
                                }

                            }
                        </tbody>
                        <tfoot>
                            <tr>
                                <td></td>
                                <td></td>
                                <td></td>
                                <td></td>
                                <td></td>
                                <td></td>
                                <td>Total Final AMT :</td>
                                <td>@Model.Sum(i => i.TotalFinalAmount) </td>
                                <td></td>
                              
                            </tr>
                        </tfoot>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>

<!--END FOOTER -->
